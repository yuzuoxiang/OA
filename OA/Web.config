<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="UploadConfig" type="FancyFix.Tools.Config.UploadConfig,Tools" />
  </configSections>
  <connectionStrings>
    <add name="OA" connectionString="Data Source=192.168.18.247,50886;User ID=sa;Password=sa_123456;Initial Catalog=OA;" />
    <!--<add name="OA" connectionString="Data Source=.;User ID=yuzuoxiang;Password=yu759974394YU;Initial Catalog=OA;" />-->
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--文本日志路径-->
    <add key="LogPath" value="~/log" />
    <add key="filePath" value="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA\Content\files" />
    <!--域名-->
    <add key="domain" value="oa.com" />
    <add key="weburl" value="http://oa.fancyfix.com" />
    <add key="imgurl" value="http://file.fancyfix.com" />
    <!--超级管理员用户Id-->
    <add key="SuperAdminIds" value="1" />
    <!--基础数据库名-->
    <add key="BaseDbName" value="OA" />
  </appSettings>
  <!--网站文件保存目录-->
  <UploadConfig>
    <SiteOptions>
      <add domain="local" url="http://oa.fancyfix.com/Content/files" folder="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA\Content\files" />
      <add domain="files" url="http://file.fancyfix.com/files" folder="E:\workspace\Fancy-fix\源代码\OA\FancyFix.OA\FancyFix.OA.Files\files" />
    </SiteOptions>
    <Settings>
      <!--网站文件-->
      <add name="file" filePath="\web\file\" urlFilePath="/web/file/" allowUpload=".rar|.zip|.doc|.docx|.xls|.xlsx|.pdf|.pdfx|.txt" maxFileSize="52428800" createSmallPic="false" width="0" height="0" createMiddlePic="false" createBigPic="false" maxWidth="0" maxHeight="0" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
      <!--网站图片-->
      <add name="web" filePath="\web\common\" urlFilePath="/web/common/" allowUpload=".gif|.jpg|.jpeg|.bmp|.png" maxFileSize="10484560" createSmallPic="true" width="250" height="250" createMiddlePic="false" createBigPic="false" maxWidth="2048" maxHeight="2048" addWaterMark="false" waterMarkType="image" waterMarkImgOrTxt="/common/images/logo.png" transparency="1" />
    </Settings>
  </UploadConfig>
  <system.web>
    <!--session状态保持-->
    <sessionState mode="StateServer" timeout="120" stateConnectionString="tcpip=127.0.0.1:42424" cookieless="false" />
    <httpCookies domain="oa.com" />
    <compilation debug="false" targetFramework="4.6.2" />
    <httpRuntime executionTimeout="240" maxQueryStringLength="2097151 " maxRequestLength="2097151 " requestValidationMode="2.0" targetFramework="4.6.2" />
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Helpers" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.WebPages" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-5.2.3.0" newVersion="5.2.3.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="WebGrease" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.5.2.14234" newVersion="1.5.2.14234" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.codedom>
    <compilers>
      <compiler language="c#;cs;csharp" extension=".cs" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.CSharpCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.7.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" warningLevel="4" compilerOptions="/langversion:6 /nowarn:1659;1699;1701" />
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.VBCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=1.0.7.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" warningLevel="4" compilerOptions="/langversion:14 /nowarn:41008 /define:_MYTYPE=\&quot;Web\&quot; /optionInfer+" />
    </compilers>
  </system.codedom>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="ApplicationInsightsWebTracking" />
    </modules>
    <security>
      <requestFiltering>
        <requestLimits maxQueryString="2147483647" maxAllowedContentLength="2147483647" />
      </requestFiltering>
    </security>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <staticContent>
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
    </staticContent>
  </system.webServer>
</configuration>
